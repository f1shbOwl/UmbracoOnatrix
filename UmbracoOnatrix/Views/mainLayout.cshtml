@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix - Financial Solutions For Your Business" : "Onatrix - Financial Solutions For Your Business";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="https://kit.fontawesome.com/c806d0e801.js" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/css/theme.css">
    <link rel="stylesheet" href="/css/blockgrid.css">
    <title>@pageTitle</title>
    
</head>
<body>
    <div class="wrapper">

        @await Html.PartialAsync("partials/static/topbar.cshtml")
        @await Html.PartialAsync("partials/static/header.cshtml")
        
        <main>
            @RenderBody()
        </main>

        @await Html.PartialAsync("partials/static/footer.cshtml")
    </div>
    

    <script src="/scripts/main.js"></script>
</body>
</html>
